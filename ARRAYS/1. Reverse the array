#include<bits/stdc++.h>
using namespace std;

int main()
{ int t;
  cout<<"Enter no. of testcases : ";
  cin>>t;
  
  while(t--)
  { string s;
    cout<<"Enter the string : ";
    cin>>s;
    
    int n = s.length();
    int start=0,end=n-1;
    
              while(start<=end)
              { swap(s[start],s[end]);
                start++;
                end--;
              }
              cout<<s<<"\n";
  }
  return 0;
}
